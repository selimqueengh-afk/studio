
{
  "rules": {
    "rules_version": "2",
    "service": "cloud.firestore",
    "match": {
      "databases": {
        "{database}": {
          "documents": {
            "{document=**}": {
              "allow": "read, write",
              "if": "request.auth != null"
            }
          }
        }
      }
    }
  }
}
